unit Udaje;

interface

const
ZiaciMax = 100;

type
pnarodnost = (Ukraina,Slovensko,Cesko,Japonsko,ine);

ppohlavie = (pMuz,pZena);

pmeno=record
  meno:string;
  priezvisko:string;
end;
pdatum=record
  den:byte;
  mesiac:byte;
  rok:word;
end;



rOsoba=record
  Meno:pmeno;
  Datumnar:pdatum;
  Pohlavie:ppohlavie;
  narodnost:pnarodnost;
end;

var
zoznamziakov: array[1..ziacimax] of rOsoba;


implementation

end.
